/*sidebar*/
ul {margin-left:30px; list-style-type:circle; }
ul.no-bullet {list-style-type:none; margin-left: 0;}
#sidemenu{
    float:left;
    width: 300px;
    background:#fff;
    border-right:1px solid #c0c0c0;
    border-bottom: 1px solid #C2C2C2;
    display:block;
}
#sidemenu ul {margin-left: 0;}
#menu-head {
margin-top: -30px;
    height: 30px;
    background: -moz-linear-gradient(100% 100% 90deg, #f2e9e1, #fff);
    background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#f2e9e1), to(#fff));
    /*background: transparent url("../images/sidemenu-header.png") repeat-x top left;*/
    border-bottom: 1px solid #C2C2C2;
    color: #1c140d;
    padding-left:10px;
    font-size: 1.2em;
    line-height:1.5em;
    font-weight: 500;
    overflow:auto;
   /* font-family: Verdana, Helvetica, Arial, sans-serif;*/
}
#menu-head h2 {
    margin:0;
}
#sidemenu li {
    list-style-type:none;
    height: 2em;
    line-height: 2em;
    color: black;
    padding-left: 5px;
    border-bottom: 1px solid #C2C2C2;
    font-size: 0.8em;
   /* font-family: "Helvetica Neue", "Lucida Grande", Helvetica, Arial, Verdana, sans-serif;*/
}
#sidemenu li.between {
 padding-left: 10px;
 background: #F8F8FF;
}

#sidemenu li.started {
    list-style-type:none;
    height: 2em;
    border-bottom: 1px solid #C2C2C2;
    font-size: 0.8em;
    background:#e0f2dc;
    /*font-family: "Helvetica Neue", "Lucida Grande", Helvetica, Arial, Verdana, sans-serif;*/
}
#sidemenu li.ended {
    list-style-type:none;
    height: 2em;
    border-bottom: 1px solid #C2C2C2;
    font-size: 0.8em;
    background:#F8F8FF;
   /* font-family: "Helvetica Neue", "Lucida Grande", Helvetica, Arial, Verdana, sans-serif;*/
}

#sidemenu li a {
    height: 2em;
    line-height:2em;
    padding-left: 5px;
    color: black;
    display: block;
    text-decoration: none;
}

#sidemenu li:hover {
    background: #F8F8FF;
}

#sidemenu #menu-hide {
    position: relative;
    top: 0;
    left: 285px;
    height: 30px;
    width: 15px;
    display: block;
    background: transparent url("<%=asset_path 'menu-hide.png'%>") no-repeat;
    cursor: pointer;
}
#sidemenu  p {
font-size: 0.8em;
padding:5px;

}
#menu-show {
    display: none;
    height: 30px;
    width: 15px;
    position: relative;
    left: -10px;
    top:-10px;
    background: transparent url("<%=asset_path 'menu-show.png'%>") no-repeat 0 0;
    cursor: pointer;
}

